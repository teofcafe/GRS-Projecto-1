ifconfig eth0 10.20.72.5 netmask 255.255.255.0 up
route add default gw 10.20.72.1 dev eth0

mkdir /var/www/mrtg

cfgmaker --global 'WorkDir:/var/www' --output /root/mrtg.cfg private@10.20.72.1
cfgmaker --global 'WorkDir:/var/www' --output /root/mrtg.cfg private@10.20.73.1
cfgmaker --global 'WorkDir:/var/www' --output /root/mrtg.cfg private@10.20.74.1
cfgmaker --global 'WorkDir:/var/www' --output /root/mrtg.cfg private@10.20.75.1

cfgmaker --global 'WorkDir:/var/www' --output /root/mrtg.cfg private@10.20.75.2

cfgmaker --global 'WorkDir:/var/www' --output /root/mrtg.cfg private@10.20.73.2

env LANG=C /usr/bin/mrtg /root/mrtg.cfg --daemon

/etc/init.d/apache2 start

indexmaker --title 'Admin Page' --output /var/www/index.html /root/mrtg.cfg
